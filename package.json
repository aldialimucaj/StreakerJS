{
  "name": "Striker",
  "version": "0.0.1",
  "description": "GitHub Strike Editor",
  "main": "lib/index.js",
  "bin": {
    "striker": "./lib/index.js"
  },
  "scripts": {
    "start": "node lib/index.js",
    "lint": "eslint src/",
    "test": "mocha --compilers js:babel-register",
    "cover": "babel-node ./node_modules/isparta/lib/cli cover node_modules/mocha/bin/_mocha -- --recursive",
    "cover_push": "babel-node ./node_modules/isparta/lib/cli cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "docs": "jsdoc -d docs src/"
  },
  "repository": {
    "type": "git",
    "url": "github.com/alimucaj/striker"
  },
  "keywords": [
    "striker",
    "github",
    "strike",
    "editor"
  ],
  "author": "Aldi Alimucaj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alimucaj/striker/issues"
  },
  "homepage": "https://github.com/alimucaj/striker",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.1",
    "jsdoc": "^3.4.0",
    "fs-extra": "^0.26.3",
    "mocha-lcov-reporter": "^1.0.0"
  },
  "dependencies": {
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "inquirer": "^0.11.1",
    "log-update": "^1.0.2",
    "mocha": "^2.3.4",
    "nodegit": "^0.6.3",
    "promise": "^7.1.1",
    "winston": "^2.1.1"
  }
}
